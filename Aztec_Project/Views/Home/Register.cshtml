<body>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="limiter">
            <div class="container-login100" style="background-image: url('../Image/bg-01.jpg');">
                <div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-54">
                    <form class="login100-form validate-form">
                        <span class="login100-form-title p-b-49">
                            Signup
                        </span>
                        @Html.ValidationSummary(true)

                        <div class="wrap-input100 validate-input m-b-23">
                            <span class="label-input100">Full Name</span><br /><br />
                            @Html.EditorFor(model => model.FullName, new { @class = "input100", @type = "text", @name = "name", @placeholder = "Enter your full name" })
                            @Html.ValidationMessageFor(model => model.FullName)
                        </div>

                        <div class="wrap-input100 validate-input m-b-23">
                            <span class="label-input100">Email Address</span><br /><br />
                            @Html.EditorFor(model => model.EmailAddress, new { @class = "input100", @type = "text", @name = "emailaddress", @placeholder = "Enter your email address" })
                            @Html.ValidationMessageFor(model => model.EmailAddress)
                        </div>

                        <div class="wrap-input100 validate-input m-b-23">
                            <span class="label-input100">Username</span><br /><br />
                            @Html.EditorFor(model => model.UserName, new { @class = "input100", @type = "text", @name = "username", @placeholder = "Set your username" })
                            @Html.ValidationMessageFor(model => model.UserName)
                        </div>

                        <div class="wrap-input100 validate-input m-b-23">
                            <span class="label-input100">Password</span><br /><br />
                            @Html.EditorFor(model => model.Password, new { @class = "input100", @type = "password", @placeholder = "Set a new password" })
                            @Html.ValidationMessageFor(model => model.Password)
                        </div>

                        <div class="wrap-input100 validate-input m-b-23">
                            <span class="label-input100">Confirm Password</span><br /><br />
                            @Html.EditorFor(model => model.ConfirmPassword, new { @class = "input100", @type = "password", @placeholder = "Confirm your password" })
                            @Html.ValidationMessageFor(model => model.ConfirmPassword)
                        </div>

                        <br />

                        <div class="container-login100-form-btn">
                            <div class="wrap-login100-form-btn">
                                <div class="login100-form-bgbtn"></div>
                                @Html.ActionLink("Signup", "Register", "Home", null, new { @class = "login100-form-btn" })
                            </div>
                        </div>

                        @if (ViewBag.Message != null)
                        {
                            <div style="border:solid 1px green">
                                @ViewBag.Message
                            </div>
                        }

                        <div class="txt1 text-center p-t-54 p-b-20">
                            <span>
                                Or Sign Up Using
                            </span>
                        </div>

                        <div class="flex-c-m">
